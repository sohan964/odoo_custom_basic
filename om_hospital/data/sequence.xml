<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1"> <!-- noupdate will help to skip next software update-->
    <!-- to generate sequence for appointment -->
    <record id="seq_hospital_appointment" model="ir.sequence">
        <field name="name">Application Reference</field>
        <field name="code">hospital.appointment</field>
        <field name="prefix">HP</field> <!-- sequence will be start with HP-->
        <field name="padding">5</field><!-- then after HP lenth will be 5-->
        <field name="company_id" eval="False"/>
    </record>

</odoo>
